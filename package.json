{
  "TODO": "Update this file with your project information",
  "name": "vite-electron-builder",
  "description": "Secure boilerplate for Electron app based on Vite",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "author": {
    "email": "kozackunisoft@gmail.com",
    "name": "Alex Kozack",
    "url": "https://kozack.me"
  },
  "repository": "https://github.com/brafdlog/caspion",
  "homepage": "https://github.com/brafdlog/caspion#README.md",
  "main": "packages/main/dist/index.js",
  "scripts": {
    "build": "yarn build:main && yarn build:preload && yarn build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production yarn build && electron-builder build --config electron-builder.yml --dir --config.asar=false",
    "test": "yarn test:main && yarn test:preload && yarn test:renderer && yarn test:e2e",
    "test:e2e": "yarn build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "yarn typecheck:main && yarn typecheck:preload && yarn typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js",
    "format": "prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\""
  },
  "devDependencies": {
    "@types/analytics-node": "^3.1.14",
    "@types/lodash": "^4.17.4",
    "@types/node": "20.14.1",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/test-utils": "2.4.6",
    "cross-env": "7.0.3",
    "electron": "30.0.9",
    "electron-builder": "24.13.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "9.26.0",
    "happy-dom": "14.12.0",
    "nano-staged": "0.8.0",
    "playwright": "1.44.1",
    "prettier": "^3.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.4.5",
    "unplugin-auto-expose": "0.3.0",
    "vite": "5.2.12",
    "vitest": "1.6.0",
    "vue": "3.4.27",
    "vue-tsc": "2.0.19"
  },
  "dependencies": {
    "@puppeteer/browsers": "^2.2.3",
    "@sentry/electron": "^5.0.0",
    "analytics-node": "^6.2.0",
    "bottleneck": "^2.19.5",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.0",
    "electron-google-oauth2": "^3.0.0",
    "electron-log": "^5.1.5",
    "electron-updater": "6.2.1",
    "emittery": "^1.0.3",
    "googleapis": "^139.0.0",
    "israeli-bank-scrapers-core": "^4.5.0",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-machine-id": "^1.1.12",
    "puppeteer-core": "^22.10.0",
    "ynab": "^1.19.0"
  }
}
